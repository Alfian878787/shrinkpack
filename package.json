{
  "name": "shrinkpack",
  "description": "Fast, resilient, reproducible builds with npm install.",
  "version": "0.18.1",
  "author": "Jamie Mason (https://github.com/JamieMason)",
  "ava": {
    "files": [
      "test/e2e/test.js"
    ]
  },
  "bin": {
    "shrinkpack": "dist/bin.js"
  },
  "bugs": "https://github.com/JamieMason/shrinkpack/issues",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "contributors": [
    "Andrew Levine (https://github.com/DrewML)",
    "Chris Wheatley (https://github.com/swirlycheetah)",
    "David Cook (https://github.com/divergentdave)",
    "Dmytro Tolstyi (https://github.com/GitAngel)",
    "Dylan Piercey (https://github.com/DylanPiercey)",
    "Emil Sågfors (https://github.com/lime)",
    "Jason Hewison (https://github.com/ChunkySamurai)",
    "Kirill Fomichev (https://github.com/fanatid)",
    "Mark Erikson (https://github.com/markerikson)",
    "Scott Sidwell (https://github.com/5id)",
    "Zlatan Vasović (https://github.com/zdroid)"
  ],
  "dependencies": {
    "bluebird": "3.5.0",
    "cacache": "9.2.9",
    "chalk": "2.1.0",
    "commander": "2.11.0",
    "execa": "0.8.0",
    "graceful-fs": "4.1.11",
    "gunzip-maybe": "1.4.1",
    "nodent-runtime": "3.0.4",
    "semver": "5.4.1",
    "when": "3.7.8"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "commit-release": "0.6.2",
    "cz-conventional-changelog": "2.0.0",
    "eslint-config-xo": "0.18.2",
    "eslint-plugin-ava": "4.2.1",
    "eslint-plugin-unicorn": "2.1.2",
    "fast-async": "6.3.0",
    "husky": "0.14.3",
    "rimraf": "2.6.1",
    "validate-commit-msg": "2.14.0",
    "xo": "0.18.2"
  },
  "engines": {
    "node": ">=4.0",
    "npm": "<3.8.8 || >3.10.3"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/shrinkpack",
  "keywords": [
    "bundle",
    "cache",
    "deploy",
    "deployments",
    "import",
    "install",
    "npm",
    "offline",
    "package",
    "save",
    "shrinkwrap"
  ],
  "license": "MIT",
  "main": "shrinkpack",
  "repository": "JamieMason/shrinkpack",
  "scripts": {
    "build": "babel --out-dir ./dist --ignore node_modules ./src",
    "commit-release": "commit-release",
    "commitmsg": "validate-commit-msg",
    "lint": "xo",
    "prebuild": "rimraf dist",
    "precommit": "npm run lint",
    "prepack": "npm run build",
    "test": "ava"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": true,
    "rules": {
      "guard-for-in": 0,
      "import/extensions": 0,
      "import/no-unassigned-import": 0,
      "no-use-before-define": 0,
      "object-curly-spacing": 0,
      "unicorn/no-process-exit": 0
    },
    "space": 2
  }
}
